      <Label fx:id="question" text="Q. " wrapText="true">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </Label>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER_LEFT" spacing="150.0">
         <children>
            <VBox>
               <children>
                  <Label fx:id="option1" text="a) " wrapText="true">
                     <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="option2" text="b) " wrapText="true">
                     <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="option3" text="c) " wrapText="true">
                     <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="option4" text="d) " wrapText="true">
                     <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <ImageView fx:id="questionrightwrong" fitHeight="84.0" fitWidth="94.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
   <effect>
      <DropShadow color="#8c8a8a" height="22.98" width="19.02" />
   </effect>
</VBox>
